(func (main)
    (define n (input))
    (define one 1)
    (define zero 0)
    (define out one)
    (define ctr zero)
    (loop
        (set ctr (+ one ctr))
        (define inner-ctr zero)
        (loop
            (set out (+ out ctr))
            (set inner-ctr (- inner-ctr one))
            (if (- ctr inner-ctr) (break))
        )
        (if (- ctr n) (break))
    )
    (return out)
)
