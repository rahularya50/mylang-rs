(func (fib x)
    (define cnt 1)
    (define p1 1)
    (define p2 1)
    (loop
        (if (- x cnt) (break))
        (define p3 (+ p1 p2))
        (set p1 p2)
        (set p2 p3)
        (set cnt (+ 1 cnt))
    )
    (return p1)
)

(func (main rs1 rs2 rd)
    (define a rs1)
    (define b rs2)
    (loop
        (if (- a b) (break))
        (set b (+ a a))
    )
)
